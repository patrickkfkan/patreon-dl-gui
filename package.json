{
  "name": "patreon-dl-gui",
  "productName": "patreon-dl-gui",
  "version": "2.4.2",
  "description": "GUI for patreon-dl",
  "type": "module",
  "main": ".vite/build/index.js",
  "scripts": {
    "prepare": "tsc --noEmit && rimraf ./resources_out && npx pkg --targets node20 --out-path ./resources_out/bin ./src/resources/patreon-dl-vimeo.js",
    "start": "npm run prepare && electron-forge start",
    "package": "cross-env-shell FORGE_PHASE=package \"npm run prepare && electron-forge package\"",
    "make": "cross-env-shell FORGE_PHASE=make \"npm run prepare && electron-forge make\"",
    "publish": "npm run prepare && electron-forge publish",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint ./src --fix",
    "prettify": "prettier --write ./src"
  },
  "author": "Patrick Kan",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickkfkan/patreon-dl-gui.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-wix": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.20.0",
    "@types/dateformat": "^5.0.3",
    "@types/lodash": "^4.17.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/yargs-parser": "^21.0.3",
    "@vitejs/plugin-react": "^4.6.0",
    "@yao-pkg/pkg": "^6.3.2",
    "cross-env": "^10.0.0",
    "css-loader": "^6.11.0",
    "electron": "^38.0.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "node-abi": "^4.14.0",
    "node-loader": "^2.1.0",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.1.0"
  },
  "dependencies": {
    "bootswatch": "^5.3.3",
    "cheerio": "^1.0.0",
    "classnames": "^2.5.1",
    "configparser": "^0.3.10",
    "dateformat": "^5.0.3",
    "env-paths": "^3.0.0",
    "html-entities": "^2.6.0",
    "lodash": "^4.17.21",
    "material-icons": "^1.13.13",
    "material-symbols": "^0.28.1",
    "normalize-url": "^8.0.1",
    "patreon-dl": "^3.4.0",
    "portfinder": "^1.0.35",
    "proxy-chain": "^2.5.8",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-custom-scrollbars-4": "^4.5.1",
    "react-data-grid": "^7.0.0-beta.56",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.0.0",
    "react-toastify": "^11.0.3",
    "react-tooltip": "^5.28.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "shescape": "^2.1.2",
    "yargs-parser": "^21.1.1"
  },
  "overrides": {
    "@electron-forge/maker-rpm": {
      "electron-installer-redhat": "git+https://github.com/electron-userland/electron-installer-redhat.git#ab6694f"
    }
  },
  "keywords": [
    "patreon",
    "download",
    "downloader"
  ]
}
